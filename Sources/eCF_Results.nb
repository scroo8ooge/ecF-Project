             SubscriptBox["a", 
              RowBox[{"N", "+", "3"}]], 
             RowBox[{
              SubscriptBox["b", 
               RowBox[{"N", "+", "3"}]], "+"}]], " ", 
            FractionBox[
             SubscriptBox["a", 
              RowBox[{"N", "+", "1"}]], 
             RowBox[{
              SubscriptBox["b", 
               RowBox[{"N", "+", "1"}]], "+"}]], " ", 
            FractionBox[
             SubscriptBox["a", 
              RowBox[{"N", "+", "2"}]], 
             RowBox[{
              SubscriptBox["b", 
               RowBox[{"N", "+", "2"}]], "+"}]], 
            FractionBox[
             SubscriptBox["a", 
              RowBox[{"N", "+", "3"}]], 
