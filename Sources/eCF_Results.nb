Cell[CellGroupData[{

Cell["Algorithm:JacobiPerronAlgorithm", "Section"],

Cell[BoxData[Cell[BoxData[
  FormBox[
   TagBox[GridBox[{
      {Cell[TextData[{
        "Given a list of ",
        Cell[BoxData[
         FormBox["d", TraditionalForm]]],
        " (",
        Cell[BoxData[
         FormBox[
          RowBox[{"d", ">", "1"}], TraditionalForm]]],
        ") real numbers ",
        Cell[BoxData[
         FormBox[
          RowBox[{"{", 
           RowBox[{
            SubscriptBox["\[Alpha]", "1"], ",", 
            SubscriptBox["\[Alpha]", "2"], ",", "\[Ellipsis]", ",", 
            SubscriptBox["\[Alpha]", "d"]}], "}"}], TraditionalForm]]],
        ", the Jacobi\[Hyphen]Perron algorithm calculates a multidimensional \
continued fraction that simultaneously approximates the given real numbers."
       }],
        LineSpacing->{1, 3}]},
      {Cell["Start setting:",
        LineSpacing->{1, 3}]},
      {Cell[BoxData[
        FormBox[
         RowBox[{
          SubsuperscriptBox["\[Alpha]", "i", 
           RowBox[{"(", "0", ")"}]], "\[LongEqual]", 
          RowBox[{
           SubscriptBox["\[Alpha]", "i"], " ", "for", " ", "1"}], 
          "\[LessEqual]", "i", "\[LessEqual]", 
          RowBox[{"d", "."}]}], TraditionalForm]],
        Evaluatable->False,
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
        ScriptLevel->0,
        SingleLetterItalics->True,
        UnderoverscriptBoxOptions->{LimitsPositioning->False},
        UnderscriptBoxOptions->{LimitsPositioning->False},
        CalculateUtilities`FormatUtilities`Private`\
UnderoverscriptBoxOptionsLimitsPositioning -> True]},
      {Cell["Define",
        LineSpacing->{1, 3}]},
