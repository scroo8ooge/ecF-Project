            RowBox[{
             SubscriptBox["q", "n"], 
             SubscriptBox["C", 
              RowBox[{"n", "-", "1"}]]}], "+", 
            RowBox[{
             SubscriptBox["p", "n"], 
             SubscriptBox["C", 
              RowBox[{"n", "-", "2"}]]}], "+", 
            SubscriptBox["C", 
