        LineSpacing->{1, 3}]},
      {Cell[TextData[{
        "\[Bullet] ",
        Cell[BoxData[
         FormBox[
          RowBox[{
           RowBox[{
            SubscriptBox["\[CurlyPhi]", "0"], "[", 
            SubscriptBox["z", "k"], "]"}], "\[LongEqual]", 
           RowBox[{"f", "(", 
            SubscriptBox["z", "k"], ")"}]}], TraditionalForm]]],
        ", ",
        Cell[BoxData[
         FormBox[
          RowBox[{"k", "\[GreaterEqual]", "0"}], TraditionalForm]]],
        "."
       }],
        LineSpacing->{1, 3}]},
      {Cell[TextData[{
        "\[Bullet] ",
        Cell[BoxData[
         FormBox[
          RowBox[{
           RowBox[{
            SubscriptBox["\[CurlyPhi]", "1"], "[", 
            RowBox[{
             SubscriptBox["z", "k"], ",", 
             SubscriptBox["z", "\[ScriptL]"]}], "]"}], "\[LongEqual]", 
           FractionBox[
            RowBox[{
             SubscriptBox["z", "\[ScriptL]"], "-", 
             SubscriptBox["z", "k"]}], 
            RowBox[{
             RowBox[{
              SubscriptBox["\[CurlyPhi]", "0"], "[", 
              SubscriptBox["z", "\[ScriptL]"], "]"}], "-", 
             RowBox[{
              SubscriptBox["\[CurlyPhi]", "0"], "[", 
              SubscriptBox["z", "k"], "]"}]}]]}], TraditionalForm]]],
        ", ",
        Cell[BoxData[
         FormBox[
          RowBox[{"\[ScriptL]", ">", "k", "\[GreaterEqual]", "0"}], 
          TraditionalForm]]],
        "."
       }],
        LineSpacing->{1, 3}]},
      {Cell[TextData[{
        "\[Bullet] ",
        Cell[BoxData[
         FormBox[
          RowBox[{
           RowBox[{
            SubscriptBox["\[CurlyPhi]", "\[ScriptL]"], "[", 
            RowBox[{
             SubscriptBox["z", "0"], ",", "\[Ellipsis]", ",", 
             SubscriptBox["z", "\[ScriptL]"]}], "]"}], "\[LongEqual]", 
           FractionBox[
            RowBox[{
             SubscriptBox["z", "\[ScriptL]"], "-", 
             SubscriptBox["z", 
              RowBox[{"\[ScriptL]", "-", "1"}]]}], 
            RowBox[{
             RowBox[{
              SubscriptBox["\[CurlyPhi]", 
               RowBox[{"\[ScriptL]", "-", "1"}]], "[", 
              RowBox[{
               SubscriptBox["z", "0"], ",", "\[Ellipsis]", ",", 
               SubscriptBox["z", 
                RowBox[{"\[ScriptL]", "-", "2"}]], ",", 
               SubscriptBox["z", "\[ScriptL]"]}], "]"}], "-", 
