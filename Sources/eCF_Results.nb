           RowBox[{"g", 
            RowBox[{"(", "x", ")"}], "y", 
            RowBox[{"(", "x", ")"}]}], "+", 
           RowBox[{"f", 
            RowBox[{"(", "x", ")"}], 
            SuperscriptBox["y", "2"], 
            RowBox[{"(", "x", ")"}]}]}]}], TraditionalForm]],
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
        ScriptLevel->0,
        SingleLetterItalics->True,
