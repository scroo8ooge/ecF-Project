        CalculateUtilities`FormatUtilities`Private`\
UnderoverscriptBoxOptionsLimitsPositioning -> True]},
      {Cell[TextData[{
        "where ",
        Cell[BoxData[
         FormBox[
          RowBox[{
           SubscriptBox["u", 
            RowBox[{"n", "+", "1"}]], "\[LongEqual]", 
           RowBox[{
            SubscriptBox["v", "n"], "-", 
            RowBox[{
             SubscriptBox["p", "n"], 
             SubscriptBox["u", "n"]}]}]}], TraditionalForm]]],
        ", ",
        Cell[BoxData[
         FormBox[
          RowBox[{
           SubscriptBox["v", 
            RowBox[{"n", "+", "1"}]], "\[LongEqual]", 
           RowBox[{
            SubscriptBox["w", "n"], "-", 
            RowBox[{
             SubscriptBox["q", "n"], 
             SubscriptBox["u", "n"]}]}]}], TraditionalForm]]],
        ", ",
        Cell[BoxData[
         FormBox[
          RowBox[{
           SubscriptBox["w", 
            RowBox[{"n", "+", "1"}]], "\[LongEqual]", 
           SubscriptBox["u", "n"]}], TraditionalForm]]],
        ", ",
        Cell[BoxData[
         FormBox[
