Cell[CellGroupData[{

Cell["Algorithm:GosperRegularContinuedFractionArithmetic", "Section"],

Cell[BoxData[Cell[BoxData[
  FormBox[
   TagBox[GridBox[{
      {Cell[TextData[{
        "Given two regular continued fraction expansions for real numbers ",
        Cell[BoxData[
         FormBox["A", TraditionalForm]]],
        " and ",
        Cell[BoxData[
         FormBox["B", TraditionalForm]]]
       }],
        LineSpacing->{1, 3}]},
      {Cell[BoxData[
        FormBox[
         RowBox[{"A", "\[LongEqual]", 
          RowBox[{
           SubscriptBox["a", "0"], "+", 
           RowBox[{
            UnderoverscriptBox[
             StyleBox["\[CapitalKappa]",
              FontSize->2 + 1.33 Inherited], 
             RowBox[{"k", "=", "1"}], 
             SubscriptBox["n", "A"]], 
            FractionBox["1", 
             SubscriptBox["a", "k"]]}]}]}], TraditionalForm]],
        Evaluatable->False,
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
        ScriptLevel->0,
        SingleLetterItalics->True,
        UnderoverscriptBoxOptions->{LimitsPositioning->False},
        UnderscriptBoxOptions->{LimitsPositioning->False},
        CalculateUtilities`FormatUtilities`Private`\
UnderoverscriptBoxOptionsLimitsPositioning -> True]},
      {Cell[BoxData[
        FormBox[
         RowBox[{"B", "\[LongEqual]", 
          RowBox[{
           SubscriptBox["b", "0"], "+", 
           RowBox[{
            UnderoverscriptBox[
             StyleBox["\[CapitalKappa]",
              FontSize->2 + 1.33 Inherited], 
             RowBox[{"k", "=", "1"}], 
             SubscriptBox["n", "B"]], 
            FractionBox["1", 
             SubscriptBox["b", "k"]]}]}]}], TraditionalForm]],
        Evaluatable->False,
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
        ScriptLevel->0,
        SingleLetterItalics->True,
        UnderoverscriptBoxOptions->{LimitsPositioning->False},
        UnderscriptBoxOptions->{LimitsPositioning->False},
        CalculateUtilities`FormatUtilities`Private`\
UnderoverscriptBoxOptionsLimitsPositioning -> True]},
      {Cell[TextData[{
        "(with ",
        Cell[BoxData[
         FormBox[
          SubscriptBox["n", "A"], TraditionalForm]]],
        " and/or ",
        Cell[BoxData[
         FormBox[
          SubscriptBox["n", "B"], TraditionalForm]]],
        " possibly \[Infinity]), an arithmetic operation ",
        Cell[BoxData[
         FormBox["f", TraditionalForm]]],
        " (addition, subtraction, multiplication, and division) can be \
carried out on the sequences of partial denominators ",
        Cell[BoxData[
         FormBox[
          SubsuperscriptBox[
           RowBox[{"{", 
            SubscriptBox["a", "k"], "}"}], 
           RowBox[{"k", "=", "0"}], 
           SubscriptBox["n", "A"]], TraditionalForm]]],
        " and ",
        Cell[BoxData[
         FormBox[
          SubsuperscriptBox[
           RowBox[{"{", 
            SubscriptBox["b", "k"], "}"}], 
           RowBox[{"k", "=", "0"}], 
           SubscriptBox["n", "B"]], TraditionalForm]]],
        " directly to obtain the partial denominators ",
        Cell[BoxData[
         FormBox[
          SubscriptBox["c", "k"], TraditionalForm]]],
        " of"
       }],
        LineSpacing->{1, 3}]},
      {Cell[BoxData[
        FormBox[
         RowBox[{"C", "\[LongEqual]", 
          RowBox[{"f", "(", 
