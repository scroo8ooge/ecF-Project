      {Cell[BoxData[
        FormBox[
         RowBox[{
          SubscriptBox["\[Xi]", "appr"], "\[LongEqual]", 
          RowBox[{
           SubscriptBox["b", "0"], "+", 
           FractionBox[
            RowBox[{"z", "-", 
             SubscriptBox["z", "0"]}], 
            RowBox[{
             SubscriptBox["b", "1"], "+", 
             FractionBox[
              RowBox[{"z", "-", 
               SubscriptBox["z", "1"]}], 
              RowBox[{
               SubscriptBox["b", "2"], "+", 
               FractionBox[
                RowBox[{"z", "-", 
                 SubscriptBox["z", "2"]}], 
