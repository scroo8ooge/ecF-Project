Cell[CellGroupData[{

Cell["Algorithm:ThieleContinuedFractionAlgorithm", "Section"],

Cell[BoxData[Cell[BoxData[
  FormBox[
   TagBox[GridBox[{
      {Cell[TextData[{
        "The Thiele continued fraction algorithm for a function ",
        Cell[BoxData[
         FormBox[
          RowBox[{"f", "(", "x", ")"}], TraditionalForm]]],
        " given ",
        Cell[BoxData[
         FormBox[
          RowBox[{"n", "+", "1"}], TraditionalForm]]],
        " distinct points ",
        Cell[BoxData[
         FormBox[
          SubscriptBox["x", "j"], TraditionalForm]]],
        ", ",
        Cell[BoxData[
         FormBox[
          RowBox[{
           RowBox[{"j", "\[LongEqual]", "0"}], ",", "1", ",", "2", ",", 
           "\[Ellipsis]", ",", "n"}], TraditionalForm]]],
        " is"
       }],
        LineSpacing->{1, 3}]},
      {Cell[BoxData[
        FormBox[
         RowBox[{
          RowBox[{
           SubscriptBox["R", "n"], "(", "x", ")"}], "\[LongEqual]", 
          RowBox[{
           RowBox[{"f", "(", 
            SubscriptBox["x", "0"], ")"}], "+", 
           RowBox[{
            UnderoverscriptBox[
             StyleBox["\[CapitalKappa]",
              FontSize->2 + 1.33 Inherited], 
             RowBox[{"j", "=", "1"}], "n"], 
            FractionBox[
             RowBox[{"x", "-", 
              SubscriptBox["x", "j"]}], 
             SubscriptBox["b", "j"]]}]}]}], TraditionalForm]],
        Evaluatable->False,
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
        ScriptLevel->0,
        SingleLetterItalics->True,
        UnderoverscriptBoxOptions->{LimitsPositioning->False},
        UnderscriptBoxOptions->{LimitsPositioning->False},
        CalculateUtilities`FormatUtilities`Private`\
UnderoverscriptBoxOptionsLimitsPositioning -> True]},
      {Cell[TextData[{
        "where the ",
        Cell[BoxData[
         FormBox[
          SubscriptBox["b", "j"], TraditionalForm]]],
        " are recursively defined through"
       }],
        LineSpacing->{1, 3}]},
      {Cell[BoxData[{
        FormBox[
         RowBox[{
          SubscriptBox["b", "j"], "\[LongEqual]", 
          RowBox[{"\[CapitalPhi]", "[", 
           RowBox[{
            SubscriptBox["x", "0"], ",", 
            SubscriptBox["x", "1"], ",", "\[Ellipsis]", ",", 
            SubscriptBox["x", "j"]}], "]"}]}], TraditionalForm], 
       "\[IndentingNewLine]", 
        FormBox[
         RowBox[{
          RowBox[{"\[CapitalPhi]", "[", 
           SubscriptBox["x", "j"], "]"}], "\[LongEqual]", 
          RowBox[{"f", "(", 
           SubscriptBox["x", "j"], ")"}]}], TraditionalForm], 
       "\[IndentingNewLine]", 
        FormBox[
         RowBox[{
          RowBox[{"\[CapitalPhi]", "[", 
           RowBox[{
            SubscriptBox["x", "0"], ",", 
            SubscriptBox["x", "1"], ",", "\[Ellipsis]", ",", 
            SubscriptBox["x", 
             RowBox[{"j", "-", "1"}]], ",", 
            SubscriptBox["x", "j"], ",", 
            SubscriptBox["x", 
             RowBox[{"j", "+", "1"}]]}], "]"}], "\[LongEqual]", 
          RowBox[{
           FractionBox[
            RowBox[{
             SubscriptBox["x", 
              RowBox[{"j", "+", "1"}]], "-", 
             SubscriptBox["x", "j"]}], 
            RowBox[{
             RowBox[{"\[CapitalPhi]", "[", 
              RowBox[{
               SubscriptBox["x", "0"], ",", 
               SubscriptBox["x", "1"], ",", "\[Ellipsis]", ",", 
               SubscriptBox["x", 
                RowBox[{"j", "-", "1"}]], ",", 
               SubscriptBox["x", 
                RowBox[{"j", "+", "1"}]]}], "]"}], "-", 
             RowBox[{"\[CapitalPhi]", "[", 
              RowBox[{
               SubscriptBox["x", "0"], ",", 
               SubscriptBox["x", "1"], ",", "\[Ellipsis]", ",", 
               SubscriptBox["x", 
                RowBox[{"j", "-", "1"}]], ",", 
               SubscriptBox["x", "j"]}], "]"}]}]], "."}]}], TraditionalForm]}],
        Evaluatable->False,
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
        ScriptLevel->0,
        SingleLetterItalics->True,
        UnderoverscriptBoxOptions->{LimitsPositioning->False},
        UnderscriptBoxOptions->{LimitsPositioning->False},
        CalculateUtilities`FormatUtilities`Private`\
UnderoverscriptBoxOptionsLimitsPositioning -> True]}
     },
     DefaultBaseStyle->"Column",
     GridBoxAlignment->{"Columns" -> {{Left}}},
     GridBoxItemSize->{"Columns" -> {{Automatic}}, "Rows" -> {{Automatic}}},
     GridBoxItemStyle->{"Columns" -> {{
          Directive[SpanMaxSize -> DirectedInfinity[1]]}}, "Rows" -> {{
          Directive[SpanMaxSize -> DirectedInfinity[1]]}}}],
    "Column"], TraditionalForm]], "Output",
  ShowCellBracket->False,
  CellMargins->{{0, 0}, {0, 0}},
