        FormBox[
         RowBox[{
          SubscriptBox["c", "k"], "\[LongEqual]", 
          RowBox[{"0", " ", "if", " ", 
           SubscriptBox["c", 
            RowBox[{"k", "+", "1"}]]}], "\[LongEqual]", 
          RowBox[{
           SubscriptBox["b", 
            RowBox[{"k", "+", "1"}]], ".", " "}]}], TraditionalForm]}],
        Evaluatable->False,
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
