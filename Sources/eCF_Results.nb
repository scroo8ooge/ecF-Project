        ","
       }],
        LineSpacing->{1, 3}]},
      {Cell[BoxData[
        FormBox[
         RowBox[{
          RowBox[{
           UnderscriptBox["lim", 
            RowBox[{"n", "\[Rule]", "\[Infinity]"}],
            LimitsPositioning->False], 
           FractionBox["1", "n"], 
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"k", "\[LongEqual]", "1"}], "n"], "f", 
           RowBox[{"(", 
            SubscriptBox["b", "k"], ")"}]}], "\[LongEqual]", 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"r", "\[LongEqual]", "1"}], "\[Infinity]"], "f", 
           RowBox[{"(", "r", ")"}], 
           FractionBox[
            RowBox[{"ln", 
             RowBox[{"{", 
              RowBox[{"1", "+", 
