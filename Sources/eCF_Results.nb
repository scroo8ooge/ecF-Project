      {Cell[BoxData[
        FormBox[
         RowBox[{
          TemplateBox[{RowBox[{
              RowBox[{
                FractionBox["1", "n"], 
                RowBox[{"ln", "(", 
                  SubscriptBox["B", "n"], ")"}]}], "-", "\[Gamma]"}]},
           "Abs"], "\[LessEqual]", 
          RowBox[{"\[Epsilon]", 
           RowBox[{"(", "n", ")"}]}]}], TraditionalForm]],
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
        ScriptLevel->0,
        SingleLetterItalics->True,
        UnderoverscriptBoxOptions->{LimitsPositioning->False},
        UnderscriptBoxOptions->{LimitsPositioning->False},
        CalculateUtilities`FormatUtilities`Private`\
UnderoverscriptBoxOptionsLimitsPositioning -> True]},
      {Cell[TextData[{
        "for almost all ",
        Cell[BoxData[
         FormBox[
          RowBox[{"t", "\[Element]", 
           RowBox[{"[", 
            RowBox[{"0", ",", "1"}]}]}], TraditionalForm]]],
        ", where ",
        Cell[BoxData[
         FormBox[
          RowBox[{"\[Epsilon]", "(", "n", ")"}], TraditionalForm]]],
        " is any positive function decreasing to zero as ",
        Cell[BoxData[
         FormBox[
          RowBox[{"n", "\[Rule]", "\[Infinity]"}], TraditionalForm]]],
        " for which ",
        Cell[BoxData[
         FormBox[
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"n", "\[LongEqual]", "1"}], "\[Infinity]"], 
           RowBox[{"1", "/", 
