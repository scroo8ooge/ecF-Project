        CalculateUtilities`FormatUtilities`Private`\
UnderoverscriptBoxOptionsLimitsPositioning -> True]},
      {Cell[BoxData[
        FormBox[
         RowBox[{
          RowBox[{
           SubscriptBox["B", "n"], "\[LongEqual]", 
           RowBox[{
            RowBox[{
             SubscriptBox["q", "n"], 
             SubscriptBox["B", 
              RowBox[{"n", "-", "1"}]]}], "+", 
            RowBox[{
             SubscriptBox["p", "n"], 
             SubscriptBox["B", 
              RowBox[{"n", "-", "2"}]]}], "+", 
            SubscriptBox["B", 
             RowBox[{"n", "-", "3"}]]}]}], ","}], TraditionalForm]],
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
        ScriptLevel->0,
