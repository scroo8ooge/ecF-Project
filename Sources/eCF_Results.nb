Cell[BoxData[Cell[BoxData[
  FormBox[
   TagBox[GridBox[{
      {Cell["Let",
        LineSpacing->{1, 3}]},
      {Cell[BoxData[
        FormBox[
         RowBox[{"\[Xi]", "\[LongEqual]", 
          TagBox[
           TagBox[
            RowBox[{
             UnderoverscriptBox[
              StyleBox["\[CapitalKappa]",
               FontSize->2 + 1.33 Inherited], 
              RowBox[{"n", "=", "1"}], "\[Infinity]"], 
             TagBox[
              FractionBox[
               SubscriptBox["a", "n"], 
               SubscriptBox["b", "n"]],
              HoldForm]}],
            HoldForm],
           HoldForm]}], TraditionalForm]],
        HyphenationOptions->{"HyphenationCharacter"->"\[Continuation]"},
