          RowBox[{
           RowBox[{
            UnderscriptBox["lim", 
             RowBox[{"n", "\[Rule]", "\[Infinity]"}],
             LimitsPositioning->False], 
            RadicalBox[
             RowBox[{
              RowBox[{
               SubscriptBox["b", "1"], "\[CenterDot]", 
               SubscriptBox["b", "2"]}], "\[CenterEllipsis]", 
              SubscriptBox["b", "n"]}], "n"]}], "\[LongEqual]", 
           RowBox[{
            UnderoverscriptBox["\[Product]", 
